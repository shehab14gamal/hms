<odoo>
    <data>





 #Action........................................................................
        <record id="doctors_action" model="ir.actions.act_window">
            <field name="name">Doctors</field>
            <field name="res_model">hms.doctor</field>
        </record>

 #view...............................................................................
        <record id="doctor_tree_view" model="ir.ui.view">
            <field name="name">doctor tree view</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="first_name"/>
                    <field name="last_name"/>
                </tree>
            </field>
        </record>

          <record id="doctor_form_view" model="ir.ui.view">
            <field name="name">doctor form view</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="image"/>

                        </group>
                        <notebook>
                            <page string ="Patients"> <field name="patient_id"/> </page>

                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>


        #menus...............................................................................
        <menuitem id="hms_doctor" name="Doctors" parent="hms_root" action="doctors_action"/>

    </data>
</odoo>